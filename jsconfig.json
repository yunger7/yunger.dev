{
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@components": ["src/components"],
			"@components/*": ["src/components/*"],
			"@hooks": ["src/hooks"],
			"@hooks/*": ["src/hooks/*"],
			"@lib/*": ["src/lib/*"],
			"@utils": ["src/utils"],
			"@utils/*": ["src/utils/*"],
			"@styles/*": ["src/styles/*"],
			"@services/*": ["src/services/*"]
		}
	}
}
